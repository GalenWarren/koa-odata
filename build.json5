{
  transpile: {

    source: "src/",
    target: "dist/",
    modules: ["common"],

    groups: {

      lib: {

        babel: {

          moduleIds: false,
          comments: false,
          compact: false,
          stage:2,
          optional: [
              "es7.decorators",
              "es7.classProperties",
              "es7.asyncFunctions"
          ],
          blacklist: [
              "regenerator"
          ]

        }

      },

      host: {

        babel: {

          moduleIds: false,
          comments: false,
          compact: false,
          stage:2,
          optional: [
              "es7.decorators",
              "es7.classProperties",
              "es7.asyncFunctions"
          ],
          blacklist: [
              "regenerator"
          ]

        }
      },

    }

  },

  debug: {

    target: "debug.js"

  },

  doc: {

    name: "api.json"

  }

}
